{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "startDate": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "datetime",
      "required": true
    },
    "city": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Berlin",
        "Munich",
        "Dortmund",
        "Dresden",
        "Frankfurt",
        "Hamburg"
      ],
      "default": "Berlin",
      "required": true
    },
    "venue": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::venue.venue",
      "inversedBy": "events"
    },
    "host": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::host.host",
      "inversedBy": "events"
    },
    "motif": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::motif.motif",
      "inversedBy": "events"
    }
  }
}
